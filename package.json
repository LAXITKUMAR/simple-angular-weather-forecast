{
  "name": "simple-angular-weather-forcast",
  "version": "1.0.0",
  "description": "Simple Angular API call to display weather of a city",
  "main": "index.html",
  "scripts": {
    "postinstall": "bower cache clean && bower install && gulp build",
    "prestart": "npm install",
    "start": "gulp serve",
    "test": "karma start",
    "test-single-run": "karma start karma.conf.js --single-run",
    "webdriver-start": "webdriver-manager update && webdriver-manager start --standalone",
    "serve-prod": "gulp serve:prod",
    "protractor-start": "wait-on http://localhost:4444 && protractor ./test/integration/protractor-conf.js",
    "cypress-run": "wait-on http://localhost:9001 && cypress open",
    "test:e2e": "npm-run-all -p -r webdriver-start serve-prod protractor-start",
    "test:cypress": "npm-run-all -p -r serve-prod cypress-run"
  },
  "devDependencies": {
    "angular-mocks": "1.6.10",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "cors": "2.8.4",
    "cypress": "2.1.0",
    "eslint": "4.19.1",
    "gulp": "4.0.2",
    "gulp-babel": "7.0.1",
    "gulp-connect": "5.5.0",
    "gulp-filter": "5.1.0",
    "gulp-jshint": "2.1.0",
    "gulp-load-plugins": "1.5.0",
    "gulp-plumber": "1.2.0",
    "gulp-uglify": "3.0.0",
    "gulp-useref": "3.1.5",
    "gulp-util": "3.0.8",
    "gulp-watch": "5.0.0",
    "jasmine-core": "3.1.0",
    "jasmine-jquery": "2.1.1",
    "jshint": "2.9.5",
    "jshint-stylish": "2.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "npm-run-all": "4.1.3",
    "open": "6.0.0",
    "phantomjs": "2.1.7",
    "protractor": "5.3.1",
    "protractor-http-mock": "0.10.0",
    "rimraf": "2.6.2",
    "run-sequence": "2.2.1",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LAXITKUMAR/"
  },
  "keywords": [
    "angular",
    "weather",
    "forcast",
    "ui"
  ],
  "author": "LAXIT KUMAR <kumar.laxit@gmail.com>",
  "license": "ISC"
}
